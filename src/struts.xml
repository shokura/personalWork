<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<constant name="struts.devMode" value="xhtml" />
	<constant name="struts.custom.i18n.resources" value="com.syokura.personalWork.property.Login"></constant>
	<package name="com.syokura.personalwork.action" extends="struts-default">


		<action name="LoginAction" class="com.syokura.presonalWork.action.LoginAction"
			method="execute">
			<result name="success">login_success.jsp</result>
			<result name="error">login.jsp</result>
		</action>

	</package>
</struts>